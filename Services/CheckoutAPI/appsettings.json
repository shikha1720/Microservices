{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "CheckoutAPI"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://eureka-server:8761/eureka/",
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true
    },
    "Instance": {
      "NonSecurePort": 80,
      "HostName": "checkoutapi",
      "InstanceId": "CheckoutAPI,Port:80",
      //"StatusPageUrlPath": "/api/Checkout",
      //"HealthCheckUrlPath": "/api/values/healthcheck"
    },
    "AllowedHosts": "*"
  }
}
